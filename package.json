{
  "name": "unikalkulus-setup",
  "version": "1.0.0",
  "description": "Setup script for UniKalkulus platform",
  "scripts": {
    "setup": "node setup.js",
    "dev": "concurrently \"npm run backend:dev\" \"npm run frontend:dev\"",
    "backend:dev": "cd backend && npm run dev",
    "frontend:dev": "npx serve -s . -l 3000",
    "backend:install": "cd backend && npm install",
    "start": "npm run backend:install && npm run dev"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "serve": "^14.2.1"
  },
  "keywords": ["education", "calculus", "learning"],
  "author": "UniKalkulus Team",
  "license": "MIT"
}